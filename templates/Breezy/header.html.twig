    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="apple-touch-icon" sizes="152x152" href="/templates/Breezy/assets/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/templates/Breezy/assets/favicon-32x32.png" sizes="32x32">
